{
  "ava": {
    "failFast": false,
    "files": [
      "dist/tests/**/*.js"
    ],
    "timeout": "1m"
  },
  "dependencies": {
    "@azure/functions": "^1.2.2",
    "@online-service/utils": "file:../utils",
    "applicationinsights": "^1.8.5",
    "lodash": "^4.17.19",
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "@hint/configuration-all": "^2.0.7",
    "@hint/utils": "^7.0.5",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.27",
    "@types/proxyquire": "^1.3.28",
    "@types/request": "^2.48.5",
    "@types/sinon": "^9.0.4",
    "@types/text-table": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "ava": "^3.11.1",
    "copyfiles": "^2.3.0",
    "debug": "^4.1.1",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-markdown": "^1.0.2",
    "globby": "^11.0.1",
    "hint": "^6.0.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "optionator": "^0.9.1",
    "proxyquire": "^2.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "text-table": "^0.2.0",
    "typescript": "^3.9.7"
  },
  "main": "./dist/src/index.js",
  "name": "@online-service/services",
  "private": true,
  "scripts": {
    "ava": "ava",
    "build": "npm run clean && npm-run-all build:*",
    "build:assets": "copyfiles \"./{src,tests}/**/{!(*.ts),.!(ts)}\" dist",
    "build:ts": "tsc",
    "prebuild": "node ./scripts/create-hint-categories.js && node ./scripts/create-hint-extends.js",
    "clean": "rimraf dist",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --ext ts --ignore-pattern dist .",
    "test": "npm run lint && npm run build && nyc ava",
    "watch:ts": "npm run build:ts -- --watch"
  },
  "version": "0.16.0"
}
